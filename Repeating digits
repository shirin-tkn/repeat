def repeat(number,digit):
    count = 0
    while number > 0:
        if digit == number % 10:
            count = count + 1
        number = number // 10
    return number

if __name__ == '__main__':
    number = int(input('Enter a number: '))
    digit = int(input('Enter a digit: '))
    print(digit,"repeats",repeat(number,digit),"times.")
